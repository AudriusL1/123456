{# templates/base.html.twig #}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% block title %}KTU events{% endblock %}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    {% block stylesheets %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
        <link rel="stylesheet" href="{{ asset('front-end/css/font-awesome.css') }}">
        <link rel="stylesheet" href="{{ asset('front-end/css/styles.css') }}">

    {% endblock %}
</head>
<body>
{% block titleBar %}
    <nav class="navbarhead-bg">
    </nav>
    <nav class="navbar navbar-expand-lg navbar-dark navbarhead-bg mb-5">
        <img src="{{ asset('front-end/images/ktu-logo-lt-pilnas.png') }}" class ="imgKTU">
        <div class="headTitleText">ACADEMIC EVENTS SYSTEM</div>

    </nav>
{% endblock %}
{% block eventBlockTitleAdmin %}

<link rel="stylesheet" href="{{ asset('front-end/css/index.css') }}">
<link rel="stylesheet" href="{{ asset('front-end/css/font-awesome.css') }}">
<link rel="stylesheet" href="{{ asset('front-end/css/styles.css') }}">


<nav class="navbarhead-bg">
</nav>
<nav class="navbar navbar-expand-lg navbar-dark navbarhead-bg mb-5">
    <img src="{{ asset('front-end/images/ktu-logo-lt-pilnas.png') }}" class ="imgKTU">
    <div class="headTitleText">ACADEMIC EVENTS SYSTEM</div>
    <link rel="stylesheet" href="{{ asset('front-end/css/linkColor.css') }}">
    <div class=" titleLinks">
        <div class="d-flex flex-row ">
            <div class="p-2">
                <td><a class="class1" href="/event/new" button >Create_new</a></td>
            </div>
            <div class="p-2">
                <td><a class="class1" href="/event">Events</a></td>
            </div>
            <div class="p-2">
                <td><a class="class1" href="/users">Users</a></td>
            </div>
            <div class="p-2">
                <td><a class="class1" href="/categories">Categories</a></td>
            </div>
            <div class="p-2">
                <td><a class="class1" href="/profile">Profile</a></td>
            </div>
            <div class="p-2">
                <td><a class="class1" href="{{path('logout')}}">Logout</a></td><br>
            </div>
        </div>
    </div>
</nav>
{% endblock %}
{% block eventBlockTitleUser %}

    <link rel="stylesheet" href="{{ asset('front-end/css/index.css') }}">
    <link rel="stylesheet" href="{{ asset('front-end/css/font-awesome.css') }}">
    <link rel="stylesheet" href="{{ asset('front-end/css/styles.css') }}">
    <link rel="stylesheet" href="{{ asset('front-end/css/linkColor.css') }}">

    <nav class="navbarhead-bg">
    </nav>
    <nav class="navbar navbar-expand-lg navbar-dark navbarhead-bg mb-5">
        <img src="{{ asset('front-end/images/ktu-logo-lt-pilnas.png') }}" class ="imgKTU">
        <div class="headTitleText">ACADEMIC EVENTS SYSTEM</div>
        <div class=" titleLinks">
            <div class="d-flex flex-row ">
                <div class="p-2">
                    <td><a class="class1" href="/event">Events</a></td>
                </div>
                <div class="p-2">
                    <td><a class="class1" href="/myevent">My_Events</a></td>
                </div>
                <div class="p-2">
                    <td><a class="class1" href="/categories">Categories</a></td>
                </div>
                <div class="p-2">
                    <td><a class="class1" href="/profile">Profile</a></td>
                </div>
                <div class="p-2">
                    <td><a class="class1" href="{{path('logout')}}">Logout</a></td><br>
                </div>
            </div>
        </div>
    </nav>
{% endblock %}


{% block meniuBar %}
{% endblock %}

{% block body %}{% endblock %}

{% block javascripts %}
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    <script>
        ('.dropdown-toggle').dropdown();
    </script>
{% endblock %}
</body>
</html>
